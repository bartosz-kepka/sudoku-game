<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>
<VBox alignment="CENTER" minWidth="330.0" minHeight="500.0" spacing="20.0"
      xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <children>
        <HBox alignment="CENTER" prefHeight="71.0" spacing="20.0">
            <children>
                <Button fx:id="cancelButton" alignment="CENTER" mnemonicParsing="false"
                        prefWidth="100.0" text="Cancel" textAlignment="CENTER"/>
                <Button fx:id="saveButton" alignment="CENTER" mnemonicParsing="false"
                        prefWidth="100.0" text="Save game" textAlignment="CENTER"/>
            </children>
            <padding>
                <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
            </padding>
        </HBox>
        <GridPane fx:id="sudokuGrid" alignment="CENTER" gridLinesVisible="true"/>
    </children>
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0"/>
    </padding>
</VBox>
